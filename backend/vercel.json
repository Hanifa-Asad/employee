{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/server.js"
    }
  ],
  "env": {
    "PORT": "5000",
    "MONGODB_URI": "your-mongodb-uri",
    "JWT_SECRET": "your-jwt-secret"
  }
}
