{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/server.js"
    }
  ],
  "env": {
    "PORT": "5000",
    "MONGODB_URI": "your-actual-uri",
    "JWT_SECRET": "ems_jwt_secret_key_2024"
  }
}
